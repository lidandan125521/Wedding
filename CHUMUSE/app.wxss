/**app.wxss**/
/*单行超出指定的范围  截取内容并添加*/
.ell{
  overflow: hidden;/*超出范围隐藏...*/
  white-space: nowrap;/*不允许换行...*/
  text-overflow: ellipsis;/*超出部分添加...*/
}
/*灰色分割快*/
.divider{
  height:30rpx;
  background:#f1f1f1;
}
/*项目条*/
.cell{
  background: #fff;
  font-size:16px;
}
.cell-item{
  padding:20rpx 30rpx;
  border-bottom:1rpx solid #ddd;
}
/*箭头*/
.arrow{
  display:flex;
  justify-content: space-between;
  align-items: center;
}
.arrow:after{
  content:"";
  width:15rpx;
  height:15rpx;
  border-top:3rpx solid #ccc;
  border-right:3rpx solid #ccc;
  transform: rotate(45deg);
}